    "gatsby-plugin-sharp": "2.6.14",
    "gatsby-source-filesystem": "2.3.14",
    "gatsby-transformer-sharp": "2.5.7",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
