  "version": "0.1.1",
  "author": "Duyet Le <me@duyet.net>",
  "scripts": {
    "lint": "eslint .",
    "build": "rm -rf public && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
