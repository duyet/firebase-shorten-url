{
  "name": "duyet-shortener",
  "private": true,
  "description": "Demo for simple shortener with Firebase",
  "version": "0.1.1",
  "author": "Duyet Le <me@duyet.net>",
  "scripts": {
    "lint": "eslint .",
    "build": "rm -rf public && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --non-interactive",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@ant-design/compatible": "1.1.0",
    "antd": "4.17.3",
    "axios": "0.24.0",
    "gatsby": "4.4.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-google-gtag": "4.4.0",
    "gatsby-plugin-manifest": "4.4.0",
    "gatsby-plugin-offline": "5.4.0",
    "gatsby-plugin-react-helmet": "5.4.0",
    "gatsby-plugin-sharp": "4.4.0",
    "gatsby-source-filesystem": "4.4.0",
    "gatsby-transformer-sharp": "4.4.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
