{
  "name": "duyet-shortener",
  "private": true,
  "description": "Demo for simple shortener with Firebase",
  "version": "0.1.1",
  "author": "Duyet Le <me@duyet.net>",
  "scripts": {
    "lint": "eslint .",
    "build": "rm -rf public && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --non-interactive",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@ant-design/compatible": "1.0.8",
    "antd": "4.8.0",
    "axios": "0.21.0",
    "gatsby": "2.25.3",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-google-gtag": "2.1.13",
    "gatsby-plugin-manifest": "2.5.2",
    "gatsby-plugin-offline": "3.3.2",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-sharp": "2.7.1",
    "gatsby-source-filesystem": "2.4.2",
    "gatsby-transformer-sharp": "2.5.21",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
