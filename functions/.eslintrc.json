{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "camelcase": [
            2,
            {
                "properties": "never"
            }
        ],
        "no-console": 0
    }
}